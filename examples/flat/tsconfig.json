{
  "compilerOptions": {
    "jsx": "react-jsx",
    "lib": ["ESNext"],
    "target": "ESNext",
    "module": "ESNext",
    "rootDir": "./",
    "moduleResolution": "Bundler",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
